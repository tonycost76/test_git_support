et(l);\r\n                if (line !=null) {\r\n                    if (line.getTax() != null) {\r\n                        salesLineTaxMap.put(salesOrderId + \"|\" + line.getUid(), line.getTax().getLineTax());\r\n                    } else {\r\n                        salesLineTaxMap.put(salesOrderId + \"|\" + line.getUid(), 0);\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        if (salesLineTaxMap.containsKey(salesOrderId + \"|\" + lineUid)){\r\n            outputLine.put(\"tax\",salesLineTaxMap.get(salesOrderId + \"|\" + lineUid));\r\n        }\r\n    }\r\n    var sku = outputLine.get(\"lineItems.systemItemId\");\r\n    if (skuUomMap.containsKey(sku)){\r\n        outputLine.put(\"uom\", skuUomMap.get(sku));\r\n    } else {\r\n        //item to get sku to get uom\r\n        var dsr = new com.eis.core.api.v1.model.DynamicSearchRequest();\r\n        var skuAtt = new com.eis.core.api.v1.model.DynamicAttribute();\r\n        skuAtt.setType(com.eis.core.api.v1.model.DynamicAttributeType.String);\r\n        skuAtt.setValue(sku);\r\n        skuAtt.setRefName(\"productIdentifier\");\r\n        dsr.getSearchFields().getAttributes().put(\"productIdentifier\", skuAtt);\r\n\r\n        var items = itemService.getList(dsr, 0, 1, null);\r\n\r\n        if (items != null && items.size() > 0){\r\n            var item = items.get(0);\r\n            if (item != null && item.getSku() != null && item.getSku().getUnitOfMeasure() != null){\r\n                var uom = item.getSku().getUnitOfMeasure().getAbbreviation();\r\n                skuUomMap.put(sku, uom);\r\n                outputLine.put(\"uom\",uom);\r\n            }\r\n        }\r\n\r\n    }\r\n}",
