costBD.setScale(2, java.math.BigDecimal.ROUND_HALF_EVEN);\r\n\r\n                                var costRef = new com.eis.core.api.v1.model.Reference();\r\n                                costRef.setName(\"cost\");\r\n                                costRef.setType(\"double\");\r\n                                costRef.setValue(costBD);\r\n                                line.getReferenceData().add(costRef);\r\n\r\n                                var termRef = new com.eis.core.api.v1.model.Reference();\r\n                                termRef.setName(\"termAllowance\");\r\n                                termRef.setType(\"double\");\r\n                                termRef.setValue(termAllowBD);\r\n                                line.getReferenceData().add(termRef);\r\n\r\n                                var damageRef = new com.eis.core.api.v1.model.Reference();\r\n                                damageRef.setName(\"damageAllowance\");\r\n                                damageRef.setType(\"double\");\r\n                                damageRef.setValue(damageAllowBD);\r\n                                line.getReferenceData().add(damageRef);\r\n                            }\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        salesOrder.getTags().addAll(tags);\r\n        salesOrder.setRefName(salesOrder.getHeader().getOrderNumber() + \":\" + java.lang.System.nanoTime());\r\n        salesOrder.getHeader().setOrderLookUpKey(salesOrder.getHeader().getOrderNumber());\r\n        salesOrder.setDataDomain(dataDomain);\r\n        salesOrder = salesOrderDAO.save(salesOrder);\r\n    }\r\n}\r\n\r\nfunction getWeight(lineItems) {\r\n    var wt = 0.0;\r\n    for (var i = 0; i < lineItems.size(); i++) {\r\n        var lineItem = lineItems.get(i);\r\n        if (LOG.isInfoEnabled)\r\n            LOG.info(\"lineItem :\" + lineItem);\r\n        var itemWt = lineItem.getDynAttributes().get(\"Weight\");\r\n        if (itemWt)\r\n            wt = wt + (itemWt * lineItem.getItemQty());\r\n    }\r\n    return wt;\r\n}\r\n\r\nfunction getCarrierServiceForWeight(wt) {\r\n    var carrierService = null;\r\n    if (wt >= 1) {\r\n        carrierService = \"USPSPriority\";\r\n    } else {\r\n        carrierService = \"USPSFirstClass\";\r\n    }\r\n    return carrierService;\r\n}\r\n\r\nfunction getCarrierServiceMain(carrier, sc, lines) {\r\n    var carrierService = carrier;\r\n    if (carrier) {\r\n        if ('1DAY' == carrier) {\r\n            carrierService = \"FedExStandardOvernight\";\r\n        } else if ('2DAY' == carrier || 'ANY' == carrier) {\r\n            carrierService = \"FedEx2Day\";\r\n        } else if ('GRD' == carrier || 'PRIOR' == carrier) {\r\n            var address = sc.getAddress();\r\n            var state = address.getState();\r\n\r\n            if ('AK' == state || 'HI' == state) {\r\n                carrierService = \"FedEx2Day\";\r\n            } else {\r\n                var wt = getWeight(lines);\r\n                if (wt >= 1) {\r\n                    var address1 = address.getAddress1().toLowerCase();\r\n                    var city = address.getCity();\r\n                    if ('APO' == city || 'FPO' == city || address1.includes('po box') || 'PR' == state || 'GU' == state || 'VI' == state) {\r\n                        carrierService = \"USPSPriority\";\r\n                    } else {\r\n                        if (\"US\" == address.getCountry()) {\r\n                            carrierService = \"FedExGroundHomeDelivery\";\r\n                        }\r\n                    }\r\n                } else {\r\n                    carrierService = \"USPSFirstClass\";\r\n                }\r\n            }\r\n        }\r\n    } else {\r\n        carrierService = getCarrierService(sc, lines);\r\n    }\r\n    return carrierService;\r\n}\r\n\r\nfunction getCarrierService(sc, lines) {\r\n    var carrierService = null;\r\n    var address = sc.getAddress();\r\n    var wt = getWeight(lines);\r\n    var city = address.getCity();\r\n    if ('APO' == city || 'FPO' == city) {\r\n        return getCarrierServiceForWeight(wt);\r\n    }\r\n\r\n    var address1 = address.getAddress1().toLowerCase();\r\n    if (address1.includes('po box')) {\r\n        return getCarrierServiceForWeight(wt);\r\n    }\r\n    var state = address.getState();\r\n    if ('PR' == state || 'GU' == state || 'VI' == state) {\r\n        return getCarrierServiceForWeight(wt);\r\n    } else if ('AK' == state || 'HI' == state) {\r\n        carrierService = \"FedEx2Day\";\r\n    } else if ('US' == address.getCountry()) {\r\n        carrierService = getCarrierServiceForWeight(wt);\r\n        if (wt >= 1) {\r\n            carrierService = \"FedExGroundHomeDelivery\";\r\n        }\r\n    }\r\n    return carrierService;\r\n}\r\n\r\n\r\nfunction mapLinesToVendor(lineItems) {\r\n    var vendorToLinesMap = new java.util.HashMap();\r\n    var size = lineItems.size();\r\n    for (var i = 0; i < size; i++) {\r\n        var orderLine = lineItems.get(i);\r\n        var skuRefName = orderLine.getSystemCatalogProductIdentifier();\r\n        var sku = skuService.getByRefName(skuRefName);\r\n        if (sku) {\r\n            orderLine.getDynAttributes().put(\"Weight\",\r\n                sku.getWeight());\r\n            // Added for GN701-122\r\n            orderLine.setItemTitle(sku.getTitle());\r\n            var referenceData = sku.getReferenceData();\r\n            var vendorId = null;\r\n            if (null != referenceData) {\r\n                var refSize = referenceData.size();\r\n                for (var j = 0; j < refSize; j++) {\r\n                    var ref = referenceData.get(j);\r\n                    if (\"PrimaryVendorId\".equals(ref.getType()) || \"PrimaryVendorId\".equals(ref.getName())) {\r\n                        vendorId = ref.getValue();\r\n                        orderLine.getDynAttributes().put(\"VendorId\",\r\n                            vendorId);\r\n\r\n                        break;\r\n                    }\r\n                }\r\n                if (null != vendorId) {\r\n                    var lines = vendorToLinesMap.get(vendorId);\r\n                    if (null == lines) {\r\n                        lines = new java.util.ArrayList();\r\n                    }\r\n                    lines.add(orderLine);\r\n                    vendorToLinesMap.put(vendorId, lines);\r\n                    var pbeRefName = vendorId + \"-\" + merchant.getRefName() + \"-12312017-12312149\" + \"-\" + skuRefName;\r\n                    var pricebookEntry = priceBookEntryService.getByRefName(pbeRefName);\r\n\r\n                    if (pricebookEntry) {\r\n                        orderLine.getDynAttributes().put(\"BaseAmount\",\r\n                            pricebookEntry.getBaseAmount());\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n    return vendorToLinesMap;\r\n}\r\n\r\nfunction processSalesOrders() {\r\n\r\n    var clazz = new java.lang.Class.forName(\"com.eis.ssit.api.v1.model.SalesOrderCollection\");\r\n    var salesOrders = mapper.readValue(body, clazz).getItems();\r\n\r\n    for (var x = 0; x < salesOrders.size(); x++) {\r\n        var salesOrder = salesOrders.get(x);\r\n        var tags = new java.util.ArrayList();\r\n        tags.add(\"orderNumber-\" + salesOrder.getHeader().getOrderNumber());\r\n        tags.add(\"orignalOrderNumber-\" + salesOrder.getHeader().getOriginalOrderNumber());\r\n        tags.add(\"transmissionId-\" + header_transmissionId);\r\n        tags.add(\"file-\" + header_CamelFileName);\r\n        tags.add(\"mailboxEntryId-\" + header_MailboxEntryId);\r\n\r\n        try {\r\n\r\n            if (!hasSalesOrderBeenReceived(salesOrder, tags)) {\r\n                java.lang.System.out.println(\"Building SalesOrder\");\r\n                buildSalesOrder(salesOrder, tags);\r\n            } else {\r\n                handleError(buildErrorList(\"Order Already Received.\"), salesOrder, false, tags);\r\n            }\r\n\r\n        } \r\n        \r\n        catch (e) {\r\n            if (LOG.isErrorEnabled()) {\r\n                LOG.error(\"Error - {}\", e.message);\r\n            }\r\n\r\n            if (e instanceof com.eis.core.api.v1.exception.AccountConfigurationException) {\r\n\r\n                handleError(buildErrorList(e.getMessage()), salesOrder, true, tags);\r\n                continue;\r\n\r\n            } else if (e.javaException) {\r\n                if (!isBlank(e.javaException.message)) {\r\n                    handleError(buildErrorList(e.javaException.message), salesOrder, true, tags)\r\n                    continue;\r\n                } else {\r\n                    throw e;\r\n                }\r\n\r\n            } else {\r\n                throw e;\r\n            }\r\n        }\r\n\r\n    }\r\n}\r\n\r\nvar merchant = getMerchant(\"GNC-Organization-\");\r\nprocessSalesOrders();",
